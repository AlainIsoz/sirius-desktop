------=_.description-216f885c-d591-38ce-8ea2-e4f8cb4d6ffa
Content-Type: text/plain
Entry-Name: .description

This test aims to check the default rules.

The test :
* Loads the tests projects released with the Sirius test product,
* Open the diagram,
* Select the Element1,
* Check the enum attribute is represented thanks to a text widget.
------=_.description-216f885c-d591-38ce-8ea2-e4f8cb4d6ffa--
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

// Select the Element1 in the diagram
with [get-editor RootDiagram | get-diagram -index 1 | get-edit-part -name RootDiagram] {
	with [get-edit-part -name Element1 | get-edit-part -className SquareEditPart] {
		mouse-move 41 73 button1 -height 100 -width 100
		mouse-press 41 73 button1 -height 100 -width 100
		mouse-hover 41 73 -height 100 -width 100
		mouse-release 41 73 button1 524288 -height 100 -width 100
	}
}

// Enum attribute is represented thanks to a radio group
with [get-view Properties | get-section Properties] {
    with [get-editbox -index 17] {
        get-property className | equals "org.eclipse.swt.custom.StyledText" | verify-true
        get-property text | equals "Enum Attribute:" | verify-true
    }
    with [get-control Any -index 63] {
        get-property className | equals "org.eclipse.eef.ide.ui.internal.widgets.RadioGroup" | verify-true
        get-property enablement | equals true | verify-true
        get-property "getChildren().Control[0].getClassName()" | equals "org.eclipse.swt.widgets.Button" | verify-true
        get-property "getChildren().Control[0].getSelection()" | equals true | verify-true
        get-property "getChildren().Control[0].getText()" | equals Literal1 | verify-true
        get-property "getChildren().Control[1].getClassName()" | equals "org.eclipse.swt.widgets.Button" | verify-true
        get-property "getChildren().Control[1].getSelection()" | equals false | verify-true
        get-property "getChildren().Control[1].getText()" | equals Literal2 | verify-true
        get-property "getChildren().Control[2].getClassName()" | equals "org.eclipse.swt.widgets.Button" | verify-true
        get-property "getChildren().Control[2].getSelection()" | equals false | verify-true
        get-property "getChildren().Control[2].getText()" | equals Literal3 | verify-true
    }
}
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
