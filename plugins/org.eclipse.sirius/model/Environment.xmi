  <defaultTools xsi:type="tool_2:ToolGroup" name="Pinning">
    <tools xsi:type="tool:ToolDescription" documentation="Mark element as pinned" name="Pin" iconPath="/org.eclipse.sirius.diagram/icons/pin.gif">
      <element name="element"/>
      <elementView name="elementView"/>
      <initialOperation>
        <firstModelOperations xsi:type="tool:ExternalJavaAction" name="Pin" id="org.eclipse.sirius.diagram.tools.internal.actions.pinning.pinElementsActionId">
          <parameters name="view" value="&lt;%$elementView%>"/>
        </firstModelOperations>
      </initialOperation>
    </tools>
    <tools xsi:type="tool:ToolDescription" documentation="Mark element as un-pinned" name="Unpin" iconPath="/org.eclipse.sirius.diagram/icons/unpin.gif">
